<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.3 -->
<interface>
  <!-- interface-name browser_app.ui -->
  <requires lib="gtk" version="4.8"/>
  <object class="GtkBox" id="header_bar">
    <property name="margin-bottom">7</property>
    <property name="margin-end">14</property>
    <property name="margin-start">14</property>
    <property name="margin-top">7</property>
    <property name="spacing">7</property>
    <child type="start">
      <object class="GtkButton" id="back_button">
        <property name="icon-name">arrow-left</property>
        <property name="tooltip-text">Back</property>
      </object>
    </child>
    <child type="start">
      <object class="GtkButton" id="forward_button">
        <property name="icon-name">arrow-right</property>
        <property name="tooltip-text">Forward</property>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="home_button">
        <property name="icon-name">go-home</property>
        <property name="tooltip-text">Home</property>
      </object>
    </child>
    <child type="start">
      <object class="GtkButton" id="reload_button">
        <property name="icon-name">view-refresh</property>
        <property name="tooltip-text">Reload</property>
      </object>
    </child>
    <child>
      <object class="GtkEntry" id="header_entry">
        <property name="hexpand">True</property>
        <property name="placeholder-text">Enter URL or search...</property>
        <property name="primary-icon-name">internet-amarok</property>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="enter_button">
        <property name="icon-name">keyboard-enter-symbolic</property>
      </object>
    </child>
    <child type="end">
      <object class="GtkMenuButton" id="header_menu">
        <property name="icon-name">overflow-menu</property>
        <property name="tooltip-text">Menu</property>
      </object>
    </child>
  </object>
</interface>
